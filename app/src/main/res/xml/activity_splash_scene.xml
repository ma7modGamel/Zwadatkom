<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="7500"
        motion:autoTransition="animateToEnd">
       <KeyFrameSet>
           <KeyAttribute
               motion:motionTarget="@+id/imgLogo"
               motion:framePosition="0"
               android:scaleX="1.2" />
           <KeyAttribute
               motion:motionTarget="@+id/imgLogo"
               motion:framePosition="0"
               android:scaleY="1.2" />
           <KeyAttribute
               motion:motionTarget="@+id/imgLogo"
               motion:framePosition="20"
               android:scaleX="0.7" />
           <KeyAttribute
               motion:motionTarget="@+id/imgLogo"
               motion:framePosition="20"
               android:scaleY="0.7" />
           <KeyAttribute
               motion:motionTarget="@+id/imgLogo"
               motion:framePosition="40"
               android:scaleX="1.1" />
           <KeyAttribute
               motion:motionTarget="@+id/imgLogo"
               motion:framePosition="40"
               android:scaleY="1.1" />
           <KeyAttribute
               motion:motionTarget="@+id/imgLogo"
               motion:framePosition="60"
               android:scaleX="1.0" />
           <KeyAttribute
               motion:motionTarget="@+id/imgLogo"
               motion:framePosition="60"
               android:scaleY="1.0" />
           <KeyAttribute
               motion:motionTarget="@+id/imgLogowhite"
               motion:framePosition="40"
               android:alpha="0" />
           <KeyAttribute
               motion:motionTarget="@+id/imgLogowhite"
               motion:framePosition="60"
               android:alpha="0.5" />
           <KeyAttribute
               motion:motionTarget="@+id/imgLogowhite"
               motion:framePosition="80"
               android:alpha="0.5" />
           <KeyAttribute
               motion:motionTarget="@+id/imgLogowhite"
               motion:framePosition="100"
               android:alpha="1" />
           <KeyAttribute
               motion:motionTarget="@+id/imgLogo2"
               motion:framePosition="40"
               android:alpha="0.2" />
           <KeyAttribute
               motion:motionTarget="@+id/imgLogo2"
               motion:framePosition="60"
               android:alpha="0" />
           <KeyAttribute
               motion:motionTarget="@+id/imgLogo2"
               motion:framePosition="80"
               android:alpha="0" />
           <KeyAttribute
               motion:motionTarget="@+id/imgLogo2"
               motion:framePosition="100"
               android:alpha="0" />
           <KeyAttribute
               motion:motionTarget="@+id/imgLogo"
               motion:framePosition="80"
               android:scaleX="50" />
           <KeyAttribute
               motion:motionTarget="@+id/imgLogo"
               motion:framePosition="80"
               android:scaleY="50" />
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>